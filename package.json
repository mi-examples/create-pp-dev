{
  "name": "@metricinsights/create-pp-dev",
  "version": "0.5.5-beta.2",
  "type": "module",
  "description": "Portal Page Templates repository",
  "bin": {
    "create-pp-dev": "index.js"
  },
  "keywords": [
    "portal",
    "page",
