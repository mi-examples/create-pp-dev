  "overrides": {
    "minimatch": "^10.2.2"
  },
  "devDependencies": {
    "@metricinsights/pp-dev": "^0.14.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/cross-spawn": "^6.0.6",
    "@types/minimist": "^1.2.5",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "prettier": "^3.8.1",
    "semantic-release": "^25.0.3",
    "unbuild": "^3.6.1"
  },
  "dependencies": {
    "@clack/prompts": "^1.0.1",
    "cross-spawn": "^7.0.6",
    "kolorist": "^1.8.0",
    "minimist": "^1.2.8"
